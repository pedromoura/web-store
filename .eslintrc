{
	"extends": "airbnb",
	"rules": {
		"no-unused-vars": [
			0
		],
		"max-len": [
			1,
			200
		],
		"new-cap": [
			0
		],
		"indent": [
			"error",
			"tab"
		],
		"react/jsx-indent-props": [
			"error",
			"tab"
		],
		"react/sort-comp": [
			1,
			{
				"order": [
					"static-methods",
					"lifecycle",
					"everything-else"
				]
			}
		]
	}
}